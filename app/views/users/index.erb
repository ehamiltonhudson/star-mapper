<div class="container">
  <div class="notification" style="background-color: #ffffff; opacity: 0.7; filter: alpha(opacity=60);">
    <h1 class="title is-1">See all Users</h1>

      <ul>
        <% @users.each do |user| %>
          <li class="title is-4"><%= link_to user.name, user_path(user) %></li>
        <% end %>
      </ul><br><br>

    <%= form_tag(users_path, method: :get) do %>
      <p class="titl is-4"> Search By Zodiac <%= text_field_tag(:search, params[:search]) %></p>
      <%= submit_tag 'Search', name: nil%>
    <% end %>
  </div>
</div>
