<h1>See all Users</h1>
  <ul>
    <% @users.each do |user| %>
      <li><%= link_to user.name, user_path(user) %></li>
      <% end %>
    </ul>

<%= form_tag(users_path, method: :get) do %>
  <p> Search By Zodiac <%= text_field_tag(:search, params[:search]) %></p>
  <%= submit_tag 'Search', name: nil %>
<% end %>
