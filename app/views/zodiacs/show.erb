<div class="star-padding container">

<h1 class="title is-1"><%=@zodiac.sign%></h1>
<p>Start Date: <%= @zodiac.start_date %> —— End Date: <%= @zodiac.end_date %></p>
<p>Defining Characteristics: <%= @zodiac.keywords %></p>
<div>---------------</div>
<p>Symbol: <%= @zodiac.symbol %></p>
<p>Element: <%= @zodiac.element %></p>
<p>Quality: <%= @zodiac.quality %></p>
<p>Vibe: <%= @zodiac.vibe %></p>
<div>---------------</div>
<p>Mental Traits: <%= @zodiac.mental_traits %></p>
<p>Pysical Traits: <%= @zodiac.physical_traits %></p>

<p>Other <%=@zodiac.sign%></p>
<ul>
  <% @zodiac.users.each do |user| %>
    <li><%= link_to user.name, user_path(user) %></li>
  <% end %>
</ul>

<div>---------------</div>
<p><%=link_to "Return to all Zodiacs", zodiacs_path %></p>

<body id='sepia'>
</body>
